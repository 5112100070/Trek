<!DOCTYPE html>
<html lang="en">

  {{ template "dashboard-header.tmpl" .}}

  <body id="page-top" data-spy="scroll" data-target=".navbar" data-offset="1">
    {{ template "dashboard-nav.tmpl" .}}

    <div id="wrapper">

      <!-- Sidebar -->
      {{ template "dashboard-sidebar.tmpl" .}}

      <div id="content-wrapper">
        <div class="container-fluid">
          
          <!-- Data table -->
          <div class="card mb-3 mt-5">
            <div class="card-header">
              <i class="fas fa-table"></i>
              Data Pengguna</div>
            <div class="card-body">
              <div class="table-responsive">
                <table class="table table-bordered" id="userTable" width="100%" cellspacing="0">
                  <thead class="text-center">
                    <tr>
                      <th class="col-md-1">No</th>
                      <th>Nama</th>
                      <th>Jenis Akun</th>
                      <th>Status</th>
                      <th>Foto</th>
                    </tr>
                  </thead>
                  <tbody class="text-center"></tbody>
                </table>
              </div>
            </div>
          </div>

        </div> <!-- /.container-fluid -->
      </div> <!-- /.content-wrapper -->
    </div> <!-- /#wrapper -->
    
    {{ template "dashboard-footer.tmpl" .}}

    {{ template "default-script.tmpl" .}}

    <script>
        $(document).ready(function(){
            initTableMember();
        });
    </script>
  </body>
</html>
